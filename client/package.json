{
  "name": "phaser3-parcel-template",
  "version": "1.0.0",
  "description": "A typescript template project for Phaser 3 using Parceljs",
  "source": "./index.html",
  "browserslist": "> 0.25%, last 2 versions, not dead",
  "scripts": {
    "dev": "parcel index.html --no-cache --config ./parcel/.parcel.dev --open",
    "build": "parcel build index.html --public-url _relativeroute_ --no-source-maps --log-level error --config ./parcel/.parcel.prod"
  },
  "author": "supertommy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ourcade/phaser3-parcel-template.git"
  },
  "homepage": "https://github.com/ourcade/phaser3-parcel-template",
  "devDependencies": {
    "@parcel/transformer-glsl": "^2.11.0",
    "@parcel/transformer-typescript-tsc": "^2.11.0",
    "@types/matter-js": "^0.19.6",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.0",
    "matter-js": "0.19.0",
    "parcel": "^2.11.0",
    "parcel-reporter-clean-dist": "^1.0.4",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "process": "^0.11.10",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "bitecs": "0.3.40",
    "phaser": "3.80.1",
    "socket.io-client": "^4.7.5"
  },
  "staticFiles": {
    "staticPath": "public"
  }
}
